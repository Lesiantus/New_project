<div class="row justify-content-center mt-3">
  <div class="col col-md-4"><h1><%= t('.header') %></h1>

    <p>
      <%= button_to t('.create_new'), new_admin_test_path, method: :get, class: 'btn btn-primary'
       %>
    </p>
    <%= render 'shared/errors', resource: @test %>




    <div class="col-12">
      <table class="table table-bordered border-primary">
        <tr>
          <th class="sort-by-title pointer">
            <%= t('.title') %>
              <%= octicon 'arrow-up', class: 'text-success hide' %>
              <%= octicon 'arrow-down', class: 'text-success hide' %>
          </th>
          <th><%= t('.level') %></th>
          <th colspan="4"><%= t('.actions') %></th>
        </tr>
        <%= render @tests %>

    </table>
  </div>
</div>
